<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Free CRM Application Test" >
    <listeners>
<listener class-name="com.automation.listner.Listner"></listener>

    </listeners>
    <test thread-count="5" name="End to End Test on chrome">
        <parameter name="browser" value="chrome"></parameter>


        <classes>
            <class name="testCases.Config"></class>
            <class name="testCases.mainPageTest"></class>
            <class name="testCases.LoginPageTest"></class>
            <class name="testCases.HomePageTest"></class>
            <class name="testCases.CheckTopsTest"></class>

        </classes>
    </test>

</suite>